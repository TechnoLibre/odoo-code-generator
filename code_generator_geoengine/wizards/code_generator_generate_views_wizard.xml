        <field name="name">Code Generator Generate Geoengine views</field>
        <field name="model">code.generator.generate.views.wizard</field>
        <field name="inherit_id" ref="code_generator.code_generator_generate_views_form" />
        <field name="arch" type="xml">
            <xpath expr="//group[@name='clear']" position="inside">
                <field name="clear_all_geoengine" />
            </xpath>
        </field>
    </record>
</odoo>
